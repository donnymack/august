{
  "extends": [
    "airbnb-base"
  ],
  "plugins": [],
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "es6": true,
    "classes": true
  },
  "rules": {
    "no-console": ["error", { "allow": ["error"] }],
    "camelcase": 0,
    "no-tabs": 0,
    "consistent-return": 0,
    "max-len": [1, 180, 4],
    "no-mixed-spaces-and-tabs": 0,
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "func-names": 0,
    "no-plusplus": 0
  }
}
